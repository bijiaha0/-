
vagrant init centos/7

vagrant up
#进入里面
vagrant ssh

# 退出
exit

vagrant status
#停止
vagrant halt

vagrant status

#删掉
vagrant destroy

y

docker-machine create demo

docker-machine ls
#进入
docker-machine ssh demo

docker version

exit

docker-machine create demo1
docker-machine ls
